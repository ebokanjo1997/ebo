def randomusers():
    while True:
        v1 = ''.join(random.choice('123546789qwertyuiop') for i in range(1))
        v2 = ''.join(random.choice('3qwertyuiop') for i in range(1))
        v3 = ''.join(random.choice('qwertyuiop') for i in range(1))
        v4 = ''.join(random.choice('qwertyuiop') for i in range(1))
        v5 = ''.join(random.choice('1234567890qwertyuiopqwertyuiopasdfghjklzxcvbnm') for i in range(1))
        v6 = ''.join(random.choice('1234567890qwertyuiopqwertyuiopasdfghjklzxcvbnm') for i in range(1))
        v7 = ''.join(random.choice('1234567890qwertyuiopqwertyuiopasdfghjklzxcvbnm') for i in range(1))
        all = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'
        simm = random.choice(all)
        defr = random.choice(all.replace(simm, ''))
        stru = simm * (6 - 1) + defr  
        hh = ''.join(random.sample(stru,6))             
        user4 = v5+'__'+v6+''+v7
        user6 = v1+''+v2+''+v7+v3
        user7 = v1+''+v2+''+v7+v4+'_'
        user8 = v1+'_'+v2+'_'+v7
        user9 = v1+'_'+v2+'_'+v3
        G = (user4,user6,user7,user8,user9)
        user = random.choice(G)
        TikTok(user)

Threads = []
for _ in range(2):
    t = threading.Thread(target=randomusers)
    t.start()
    Threads.append(t)

for thread in Threads:
    thread.join()
